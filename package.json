{
	"name": "webauthn",
	"version": "0.0.1",
	"private": true,
	"config": {
		"bld": "node_exec npx vite build -m production",
		"chk": "node_exec npx sv check",
		"cln": "clean node_modules .svelte_kit/output",
		"exec": "npx tsx npm/run",
		"nci": "node_exec npm ci",
		"prd_prms": "--cors --host www1.pcpllc.us --port 8443 -l info",
		"rslt_ps": "$( if ( $? -ne 0 ) { exit } )"
	},
	"scripts": {
		"build": "$npm_package_config_exec $npm_package_config_cln && $npm_package_config_exec $npm_package_config_nci && $npm_package_config_exec $npm_package_config_chk && ([ $? -ne 0 ] && return) && $npm_package_config_exec $npm_package_config_bld",
		"check": "$npm_package_config_exec $npm_package_config_chk",
		"ci": "$npm_package_config_exec $npm_package_config_nci",
		"clean": "$npm_package_config_exec $npm_package_config_cln",
		"dev": "$npm_package_config_exec $npm_package_config_cln && $npm_package_config_exec $npm_package_config_nci && $npm_package_config_exec $npm_package_config_chk && ([ $? -ne 0 ] && return) && $npm_package_config_exec node_exec_detach npx vite dev",
		"run": "$npm_package_config_exec node_exec_detach node_modules/.bin/vite -m production $npm_package_config_prd_prms",
		"prod": "$npm_package_config_exec $npm_package_config_cln && $npm_package_config_exec $npm_package_config_nci && $npm_package_config_exec $npm_package_config_chk && ([ $? -ne 0 ] && return) && $npm_package_config_exec $npm_package_config_bld && ([ $? -ne 0 ] && return) && $npm_package_config_exec node_exec_detach node_modules/.bin/vite -m production $npm_package_config_prd_prms",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"preview": "vite preview",
		"sk:check": "svelte-check --tsconfig ./tsconfig.json",
		"sk:sync": "svelte-kit sync",
		"sk:sync-check-watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@types/better-sqlite3": "^7.6.11",
		"prettier": "^3.1.1",
		"svelte-check": "^4.3.3"
	},
	"type": "module",
	"dependencies": {
		"@mysql/xdevapi": "^8.0.35",
		"@node-rs/argon2": "^1.8.3",
		"@oslojs/binary": "^1.0.0",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@oslojs/otp": "^1.0.0",
		"@oslojs/webauthn": "^1.0.0",
		"@pilcrowjs/db-query": "^0.0.2",
		"@pilcrowjs/object-parser": "^0.0.4",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.46.5",
		"arctic": "^3.7.0",
		"better-sqlite3": "^12.0.0",
		"cors": "^2.8.5",
		"svelte": "^5.40.0",
		"typescript": "^5.9.3",
		"uqr": "^0.1.2",
		"valibot": "^1.1.0",
		"vite": "^7.1.10"
	},
	"overrides": {
		"@sveltejs/kit": {
			"cookie": "^1.0.2"
		}
	}

}
